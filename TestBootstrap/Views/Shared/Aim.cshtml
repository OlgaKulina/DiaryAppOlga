

@model IEnumerable<TestBootstrap.Models.UserAim>


@{
    ViewData["Title"] = "Aim";
}

<h1>Aim</h1>

@*<a class="alert-light" asp-action="CreateAim">Add the aim</a>*@
<table class="table table-striped">
    <tr><td>Aim</td><td>Description</td><td>StartDate</td><td>EndDate</td></tr>
    @foreach (var item in Model)
    {
        <tr><td>@item.Aim</td><td>@item.Description</td><td>@item.StartDate</td><td>@item.EndDate </td></tr>
    }
</table>

<a class="badge-primary" asp-action="CreateAim"> Set aim </a>

